  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot --content-base src/",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "eslint:check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "npm run lint && npm test",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "test:overwrite:snap": "npm test -- -u",
    "coverage:open:mac": "open coverage/lcov-report/index.html",
    "coverage:open:linux": "xdg-open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "vue": "^2.5.11",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.7.0",
    "file-loader": "^1.1.4",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-vue-preprocessor": "^1.4.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.13.7",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
